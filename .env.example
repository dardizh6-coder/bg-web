APP_ENV=production
APP_SECRET=change-me

# Background removal model (rembg). Default: u2net (quality).
# Examples: u2net, u2netp, isnet-general-use
RMBG_MODEL=u2net

# Stripe (optional until you enable payments)
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
STRIPE_PRICE_CHF_199=
PUBLIC_BASE_URL=http://localhost:8000

# Admin
ADMIN_PASSWORD=change-me

# Allow frontend hosted elsewhere (comma-separated)
# Example: CORS_ORIGINS=https://rbg.aucto.ch,https://www.rbg.aucto.ch
CORS_ORIGINS=

# Optional AdSense (frontend uses as placeholder config)
ADSENSE_CLIENT=
ADSENSE_SLOT=
