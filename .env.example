APP_ENV=production
APP_SECRET=change-me

# Optional: shared API key for simple auth (Railway). If set, frontend sends Authorization: Bearer <API_KEY>.
# When using API_KEY, set CORS_ORIGINS=* or your frontend origin(s).
API_KEY=

# Background removal model (rembg). Default: u2net (quality).
# Examples: u2net, u2netp, isnet-general-use
RMBG_MODEL=u2net

# Stripe (optional until you enable payments)
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
STRIPE_PRICE_CHF_199=
PUBLIC_BASE_URL=https://zhaku.eu

# Admin
ADMIN_PASSWORD=change-me

# Allow frontend hosted elsewhere (comma-separated)
# Example: CORS_ORIGINS=https://zhaku.eu,https://www.zhaku.eu
CORS_ORIGINS=

# Optional AdSense (frontend uses as placeholder config)
ADSENSE_CLIENT=
ADSENSE_SLOT=
